@model UchetAuto.Models.TypeMotor

<div class="modal-header">
    <h5> Редактирование типа двигателя</h5>
</div>
<div class="modal-body">

    <label>ID записи</label>
    <input type="text" class="form-control btn-block" id="ID" value="@Model.IDTypeMotor" />

    <label>Тип двигателя</label>
    <input type="text" class="form-control btn-block" id="TypeMotor" value="@Model.TypeMotor1" />

    <label>Признак</label>
    <input type="text" class="form-control btn-block" id="Priznak" value="@Model.Priznak" />



</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="TypeMotorEditSave(@Model.IDTypeMotor)">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>





