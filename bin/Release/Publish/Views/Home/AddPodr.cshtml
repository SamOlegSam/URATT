@model UchetAuto.Models.Podrazd

<div class="modal-header">
    <h5> Создание подразделения</h5>
</div>
<div class="modal-body">


    <label>Подразделение</label>
    <input type="text" class="form-control btn-block" id="Podrazd" />

    <label>Филиал</label>
    @Html.DropDownList("Filial1", ViewBag.fil as SelectList, "Выберите филиал", new { @class = "form-control btn-block", id = "Fil" })

    <label>Признак</label>
    <input type="text" class="form-control btn-block" id="Priznak" />
</div>


<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="PodrAddSave()">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>



