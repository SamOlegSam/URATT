@model UchetAuto.Models.Doljnost

<div class="modal-header">
    <h5> Редактирование должности</h5>
</div>
<div class="modal-body">

    <label>ID записи</label>
    <input type="text" class="form-control btn-block" id="ID" value="@Model.IDDolj" />

    <label>Должность</label>
    <input type="text" class="form-control btn-block" id="Doljnost" value="@Model.Doljnost1" />

    <label>Признак</label>
    <input type="text" class="form-control btn-block" id="Priznak" value="@Model.Priznak" />



</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="DoljEditSave(@Model.IDDolj)">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>




